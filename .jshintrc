{
  // Details: https://github.com/victorporof/Sublime-JSHint#using-your-own-jshintrc-options
  // Example: https://github.com/jshint/jshint/blob/master/examples/.jshintrc
  // Documentation: http://www.jshint.com/docs/
  "browser": true,
  "curly":false,
  "eqeqeq":true,
  "esnext": true,
  "globalstrict": true,
  "indent":false,
  "latedef":true,
  "newcap": true,
  "nonew": true,
  "quotmark": false,
  "smarttabs": true,
  "trailing": true,
  "undef": false,
  "unused": true,
  "strict": false,
  "globals": {
    "module": true,
    "require": true
  }
}
